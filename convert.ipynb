   "source": [
    "cmap_name = 'rpb_lovely'\n",
    "\n",
    "old = open(cmap_name+'_old.txt','r')\n",
    "new = open(cmap_name+'.txt','w')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "21f830b1",
   "metadata": {},
   "outputs": [],
   "source": [
    "for line in old:\n",
    "    val = line.split()\n",
    "    new.write(str(float(val[0])/255)+'\\t'+str(float(val[1])/255)+'\\t'+str(float(val[2])/255)+'\\n')\n",
    "old.close()\n",
    "new.close()"
   ]
  },
